<%- include("partials/header") -%>
<!-- <div class=""> -->

<form class="form"  action="/compose" method="post">
  <h1>Compose the new Post</h1><br>
<label for="">Title</label>
<br>
<input type="text" name="title" value="" size="120">
<br>
<label for="">Content</label>
<br>
<textarea name="Content" rows="6" cols="123"></textarea>
<br>
<label for="">Image-1</label>
<br>
<input type="text" name="img1" value="" size="120">
<br>
<label for="">Content -2</label>
<br>
<textarea name="Content_2" rows="6" cols="123"></textarea>
<br>
<label for="">Image-2</label>
<br>
<input type="text" name="img2" value="" size="120">
<br>
<label for="">Further Refer</label>
<br>
<input type="text" name="refer" value="" size="120">
<br>
<button class="btn btn-primary" type="submit" name="button">Post</button>
</form>
<%- include("partials/footer") -%>
